\ProvidesPackage{beamerthemefau-4-3}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\RequirePackage{ifthen}
\RequirePackage{geometry}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newif\iffau@isfau\fau@isfautrue
\newif\iffau@isphil\fau@isphilfalse
\newif\iffau@isrewi\fau@isrewifalse
\newif\iffau@ismed\fau@ismedfalse
\newif\iffau@isnat\fau@isnatfalse
\newif\iffau@istech\fau@istechfalse
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\DeclareOptionBeamer{institute}{%
  \PassOptionsToPackage{institute=#1}{beamercolorthemefau}
  \ifthenelse{\equal{#1}{fau}}{\fau@isfautrue}{\fau@isfaufalse}%
  \ifthenelse{\equal{#1}{phil}}{\fau@isphiltrue}{\fau@isphilfalse}%
  \ifthenelse{\equal{#1}{rewi}}{\fau@isrewitrue}{\fau@isrewifalse}%
  \ifthenelse{\equal{#1}{med}}{\fau@ismedtrue}{\fau@ismedfalse}%
  \ifthenelse{\equal{#1}{nat}}{\fau@isnattrue}{\fau@isnatfalse}%
  \ifthenelse{\equal{#1}{tech}}{\fau@istechtrue}{\fau@istechfalse}%
}
\DeclareOptionBeamer{variant}{%
  \PassOptionsToPackage{variant=#1}{beamercolorthemefau}
}
\DeclareOptionBeamer{logo}{%
  \PassOptionsToPackage{userlogo=#1}{beamerinnerthemefau}
  \PassOptionsToPackage{userlogo=#1}{beamerouterthemefau}
}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
